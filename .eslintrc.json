{
  "extends": ["airbnb"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-props-no-spreading": "off",
    "object-curly-newline": "off",
    "jsx-a11y/label-has-associated-control": [ "error", {
      "required": {
        "some": [ "nesting", "id"  ]
      }
    }],
    "jsx-a11y/label-has-for": [ "error", {
      "required": {
        "some": [ "nesting", "id"  ]
      }
    }]
  }
}
